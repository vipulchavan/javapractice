
public class Main
{
  public static void main (String[]args)
  {
    for (int row = 1; row <= 5; ++row)
      {
	for (int j = 5 - row; j >= 1; --j)
	  {
	    System.out.print (" ");
	  }
	for (int k = 1; k <= row; ++k)
	  {
	    System.out.print ("*");
	  }
	  if (row >= 2){
	      	for (int k = 1; k <= row-1; ++k)
	    {
	    System.out.print ("*");
	    }
	  }
	for (int j = 5 - row; j >= 1; --j)
	  {
	    System.out.print (" ");
	  }
	System.out.println ();
      }
  }
}
